#layer {
   text-name: [name];
   text-face-name: "Arial";
   text-halo-fill: #ffffff;
   text-halo-radius: 4;
   text-halo-bgsmooth: true;
   text-halo-bgsmooth-min: #000000;
   text-halo-bgsmooth-max: #ffffff;
   text-halo-bgsmooth-outlier-lotrim: 0.2;
   text-halo-bgsmooth-outlier-hitrim: 0.2;
}
